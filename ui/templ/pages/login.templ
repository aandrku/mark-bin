package pages

import "github.com/aandrku/mark-bin/ui/templ/layout"

templ Login() {
	@layout.Login() {
		<div>
			<form>
				<input id="email" placeholder="email"/>
				<input id="password" placeholder="password"/>
				<button>Don' have an account?</button>
				<a href="/signup">Sign up</a>
			</form>
		</div>
	}
}
